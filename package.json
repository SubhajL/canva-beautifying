{
  "name": "beautifyai-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 7071",
    "dev:ui": "next dev -p 7072",
    "build": "npm run validate:env && next build",
    "start": "next start -p 5000",
    "start:ui": "next start -p 5002",
    "lint": "next lint",
    "validate:env": "tsx scripts/validate-env.ts",
    "validate:env:prod": "NODE_ENV=production tsx scripts/validate-env.ts",
    "generate:secrets": "tsx scripts/generate-secrets.ts",
    "test:r2": "node scripts/test-r2.js",
    "test:supabase": "node scripts/test-supabase.js",
    "test:ai": "node scripts/test-ai.js",
    "test:redis": "jest --config jest.config.redis.js",
    "test:image-generation": "tsx scripts/test-image-generation.ts",
    "test:images": "tsx scripts/test-image-generation-enhanced.ts",
    "test:emails": "tsx scripts/test-emails.tsx",
    "test:export": "tsx scripts/test-export.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='(?<!integration|load|e2e)\\.test\\.ts$'",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:integration:ui": "COMPOSE_PROJECT_NAME=ui jest --config=jest.integration.config.js",
    "test:with-redis": "./scripts/test-with-redis.sh",
    "redis:start": "docker-compose -f docker-compose.test.yml up -d",
    "redis:stop": "docker-compose -f docker-compose.test.yml down",
    "test:load": "jest --testMatch='**/*.load.test.ts' --runInBand",
    "test:chaos": "jest --testMatch='**/chaos/*.test.ts' --runInBand",
    "test:e2e": "tsx scripts/pw-wrapper.ts",
    "test:e2e:always-debug": "PWDEBUG=1 playwright test --ui",
    "test:e2e:yolo": "PW_HEARTBEAT_EXIT_AFTER_MS=180000 PW_WATCHDOG_IDLE_MS=120000 tsx scripts/pw-watchdog.ts --config=playwright.yolo.config.ts",
    "test:e2e:yolo:shards": "PW_SHARDS=6 tsx scripts/pw-run-shards.ts --config=playwright.yolo.config.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "PWDEBUG=1 tsx scripts/pw-wrapper.ts",
    "test:e2e:critical": "tsx scripts/pw-wrapper.ts critical-issues",
    "test:e2e:a11y": "tsx scripts/pw-wrapper.ts accessibility",
    "test:e2e:a11y:yolo": "tsx scripts/pw-wrapper.ts --config=playwright.yolo.config.ts --project=accessibility e2e/accessibility",
    "test:e2e:perf": "tsx scripts/pw-wrapper.ts performance",
    "test:e2e:flow": "tsx scripts/pw-wrapper.ts enhancement-flow",
    "test:e2e:subscription": "tsx scripts/pw-wrapper.ts subscription-tiers",
    "test:e2e:batch": "tsx scripts/pw-wrapper.ts batch-processing",
    "test:e2e:errors": "tsx scripts/pw-wrapper.ts error-handling",
    "test:e2e:export": "tsx e2e/run-export-tests.ts",
    "test:e2e:export:focused": "tsx scripts/pw-wrapper.ts e2e/tests/results-export-focused.spec.ts",
    "test:e2e:export:smoke": "tsx scripts/pw-wrapper.ts e2e/tests/export-flow-smoke.spec.ts",
    "test:perf": "tsx scripts/pw-wrapper.ts --config=playwright.performance.config.ts",
    "test:perf:basic": "tsx scripts/pw-wrapper.ts e2e/performance/basic-metrics.spec.ts",
    "test:perf:journeys": "tsx scripts/pw-wrapper.ts e2e/performance/critical-journeys.spec.ts",
    "test:perf:full": "tsx scripts/pw-wrapper.ts --config=playwright.performance.config.ts e2e/performance/comprehensive-test.spec.ts",
    "test:perf:ci": "tsx scripts/pw-wrapper.ts --config=playwright.performance.config.ts --reporter=json",
    "lighthouse": "tsx scripts/run-lighthouse.ts",
    "lighthouse:ci": "lhci autorun",
    "test:e2e:watchdog": "tsx scripts/pw-wrapper.ts e2e/tests/watchdog.spec.ts --project=chromium --max-failures=1",
    "test:e2e:setup": "tsx scripts/setup-test-users.ts",
    "test:e2e:cleanup": "tsx e2e/setup-test-env.ts cleanup",
    "test:e2e:auth": "tsx scripts/generate-auth-storage.ts",
    "test:e2e:comprehensive": "ts-node e2e/run-comprehensive-test.ts",
    "test:e2e:visual": "ts-node e2e/visual-test-runner.ts",
    "test:e2e:report": "playwright show-report",
    "trunc": "bash bin/trunc.sh",
    "compact:pw": "tsx scripts/compact-pw.ts test-results/pw.json test-results/pw.compact.json",
    "ci:grab:a11y": "bash scripts/ci/grab-a11y-artifacts.sh",
    "test:e2e:codegen": "playwright codegen http://localhost:5000",
    "test:e2e:staging": "STAGING_URL=$STAGING_URL tsx scripts/pw-wrapper.ts",
    "test:production": "jest --testMatch='**/*.prod.test.ts'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "workers": "tsx scripts/start-workers.ts",
    "workers:ui": "PORT=5007 tsx scripts/start-workers.ts",
    "workers:dev": "tsx watch scripts/start-workers.ts",
    "workers:export": "tsx scripts/start-export-worker.ts",
    "workers:export:dev": "tsx watch scripts/start-export-worker.ts",
    "websocket": "node -r ./scripts/load-env.js -r tsx/cjs scripts/websocket-server.ts",
    "websocket:ui": "WS_PORT=5006 node -r ./scripts/load-env.js -r tsx/cjs scripts/websocket-server.ts",
    "websocket:dev": "tsx watch scripts/websocket-server.ts",
    "start:all": "tsx scripts/start-services.ts",
    "start:services": "tsx scripts/start-services.ts",
    "docker:up": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.override.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.override.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.override.yml logs -f",
    "docker:build": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.override.yml build",
    "docker:redis:init": "bash docker/scripts/init-redis.sh",
    "docker:monitoring": "docker-compose -f docker/docker-compose.monitoring.yml up -d",
    "docker:monitoring:down": "docker-compose -f docker/docker-compose.monitoring.yml down",
    "health": "curl -f http://localhost:5000/api/health || exit 1",
    "terraform:init": "cd terraform && terraform init",
    "terraform:plan": "cd terraform && terraform plan",
    "k8s:validate": "kubectl apply --dry-run=client -f k8s/",
    "monitoring:dashboard": "tsx scripts/generate-dashboards.ts"
  },
